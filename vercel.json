{
  "version": 2,
  "builds": [
    {
      "src": "apps/dev-converter/package.json",
      "use": "@vercel/next",
      "config": {
        "installCommand": "cd ../.. && pnpm install",
        "buildCommand": "cd ../.. && pnpm --filter dev-converter build",
        "outputDirectory": "apps/dev-converter/.next"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/apps/dev-converter/$1"
    }
  ]
}